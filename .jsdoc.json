{
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/minami"
  },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["server/", "plugins", "src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
