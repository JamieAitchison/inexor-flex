{
  "name": "@inexor-game/flex",
  "version": "0.9.0",
  "description": "Scripting environment for Inexor.",
  "scripts": {
    "reinstall": "npm install @inexor-game/api @inexor-game/console @inexor-game/context @inexor-game/entities @inexor-game/gameclient @inexor-game/gameserver @inexor-game/instances @inexor-game/interfaces @inexor-game/logger @inexor-game/logging @inexor-game/media @inexor-game/path @inexor-game/profiles @inexor-game/tree @inexor-game/treeclient @inexor-game/types @inexor-game/loader",
    "start": "node server/index.js --hostname $npm_package_config_hostname --port $npm_package_config_port --level $npm_package_config_loglevel --ingorepid $npm_package_config_ignorepid",
    "stop": "./inexor flex shutdown",
    "restart": "./inexor flex restart",
    "test": "mocha $(find . -not -iwholename '*node_modules*' -iwholename '*test*' -name '*.js')",
    "docs": "jsdoc -c .jsdoc.json",
    "lint": "eslint .",
    "dev": "node server/index.js --profile=devenv",
    "server": "node server/index.js --profile=server",
    "shell": "node inexor shell",
    "build": "npm run build:ui-flex",
    "build:ui-flex": "cd interfaces/ui-flex && npm run build",
    "build:ui-client-hud": "cd interfaces/ui-client-hud && npm run build",
    "build:ui-console": "cd interfaces/ui-console && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inexorgame/flex.git"
  },
  "keywords": [
    "inexor",
    "gaming"
  ],
  "author": "The Inexor Team",
  "license": "Zlib",
  "bugs": {
    "url": "https://github.com/inexorgame/code/issues"
  },
  "homepage": "https://github.com/inexorgame/flex#readme",
  "engines": {
    "node": ">=6.9.1"
  },
  "dependencies": {
    "@inexor-game/api": "file:server/api",
    "@inexor-game/console": "file:src/console",
    "@inexor-game/context": "file:src/context",
    "@inexor-game/entities": "file:src/entities",
    "@inexor-game/gameclient": "file:src/gameclient",
    "@inexor-game/gameserver": "file:src/gameserver",
    "@inexor-game/instances": "file:src/instances",
    "@inexor-game/interfaces": "file:src/interfaces",
    "@inexor-game/loader": "file:src/loader",
    "@inexor-game/logger": "file:server/util/logger",
    "@inexor-game/logging": "file:src/logging",
    "@inexor-game/media": "file:src/media",
    "@inexor-game/path": "file:server/util/path",
    "@inexor-game/profiles": "file:src/profiles",
    "@inexor-game/tree": "file:src/tree",
    "@inexor-game/treeclient": "file:src/treeclient",
    "@inexor-game/types": "file:src/types",
    "@inexor-ui/ui-flex": "file:interfaces/ui-flex",
    "circular-json": "^0.3.1",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "express-ws": "^3.0.0",
    "figlet": "^1.2.0",
    "npid": "^0.4.0",
    "portscanner": "^2.1.1",
    "prettyjson": "^1.2.1",
    "segfault-handler": "^1.0.0",
    "toml": "^2.3.2",
    "tomlify": "^0.2.0",
    "wait-on": "^2.0.2",
    "xdg-basedir": "^3.0.0",
    "yargs": "^7.0.2",
    "yarn": "^0.24.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-iterator": "^1.1.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-html": "^3.1.0",
    "eslint-plugin-import": "^2.7.0",
    "jsdoc": "^3.4.3",
    "minami": "^1.1.1",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0"
  },
  "config": {
    "port": "31416",
    "hostname": "localhost",
    "loglevel": "info",
    "ignorepid": false
  }
}
